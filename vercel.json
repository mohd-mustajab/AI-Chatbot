{
  "builds": [
    {
      "src": "api/dialogflow.js",
      "use": "@vercel/node"
    },
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/dialogflow.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/dist/$1"
    }
  ]
}
