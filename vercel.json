{
  "buildCommand": "cd Frontend && npm install && npm run build && cp -r dist ../dist",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
